<% @electricians.each do |electrician| %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"><%= electrician.firstname.capitalize %></h3>
    </div>
    <div class="panel-body">
      <div class='row'>
        <div class="col-xs-4">
          Avatar
        </div>
        <div class="col-xs-6">
          <p><%= electrician.license_num %></p>
          <p><%= electrician.experience %></p>
          <p><%= electrician.tel_num %></p>
        </div>
        <div class="col-xs-2">
          Button
        </div>     
      </div>
    </div>
  </div>
<% end %>